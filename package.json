{
  "name": "geopoio",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "description": "GeoPOIO - Discover Points of Interest along your GPX routes and around your location",
  "author": "GeoPOIO",
  "license": "MIT",
  "homepage": "https://fxi.io/geopoio",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fxi/geopoio.git"
  },
  "bugs": {
    "url": "https://github.com/fxi/geopoio/issues"
  },
  "keywords": [
    "geopoio",
    "poi",
    "points-of-interest",
    "gpx",
    "map",
    "route",
    "geolocation",
    "svelte",
    "maplibre",
    "overpass"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json && tsc -p tsconfig.node.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "svelte-check --tsconfig ./tsconfig.json",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist node_modules/.vite",
    "deploy": "npm run build && gh-pages -d dist"
  },
  "dependencies": {
    "@turf/turf": "^7.2.0",
    "gpx-parser-builder": "^1.1.1",
    "maplibre-gl": "^5.6.1"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^5.1.0",
    "@tsconfig/svelte": "^5.0.4",
    "@types/geojson": "^7946.0.16",
    "gh-pages": "^6.2.0",
    "svelte": "^5.34.9",
    "svelte-check": "^4.2.2",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3",
    "vite": "^6.3.5"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
